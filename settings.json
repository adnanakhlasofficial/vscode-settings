{
  "editor.copyWithSyntaxHighlighting": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.emptySelectionClipboard": false,
  "window.newWindowDimensions": "inherit",
  "editor.multiCursorModifier": "alt",
  "files.trimTrailingWhitespace": true,
  "diffEditor.renderSideBySide": false,
  "editor.snippetSuggestions": "top",
  "editor.detectIndentation": false,
  // "window.nativeFullScreen": true,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  // "workbench.editor.showTabs": "none",
  "editor.minimap.enabled": false,
  "editor.lineNumbers": "on",
  "editor.guides.indentation": false,
  "workbench.tree.renderIndentGuides": "none",

  "scm.diffDecorations": "none",
  "editor.hover.delay": 500,
  "editor.hover.enabled": true,
  "editor.matchBrackets": "never",
  "workbench.tips.enabled": false,
  "editor.colorDecorators": false,
  "git.decorations.enabled": false,
  "editor.lightbulb.enabled": "off",
  "editor.selectionHighlight": false,
  "editor.overviewRulerBorder": false,
  "editor.renderLineHighlight": "none",
  "editor.occurrencesHighlight": "off",
  "problems.decorations.enabled": false,
  "editor.renderControlCharacters": false,
  "editor.hideCursorInOverviewRuler": true,
  "editor.gotoLocation.multipleReferences": "goto",
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleDeclarations": "goto",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "editor.gotoLocation.multipleImplementations": "goto",
  "editor.gotoLocation.multipleTypeDefinitions": "goto",

  "vscode_custom_css.imports": [
    "file:///C:/Users/adnan/Documents/vscode-settings/custom-vscode.css",
    "file:///C:/Users/adnan/Documents/vscode-settings/vscode-script.js"
  ],

  "editor.wordWrap": "on",
  "editor.minimap.autohide": "mouseover",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "workbench.activityBar.location": "bottom",
  "editor.fontFamily": "JetBrains Mono",
  "editor.fontSize": 16,
  "editor.fontLigatures": "'calt', 'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'zero', 'onum'",
  "editor.fontWeight": "600",
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": ["keyword.control", "storage.type", "keyword.operator"],
        "settings": {
          "fontStyle": "italic"
        }
      }
    ]
  },
  "editor.semanticTokenColorCustomizations": {
    "rules": {
      "keyword": {
        "fontStyle": "italic"
      },
      "type": {
        "fontStyle": "italic"
      }
    }
  },
  "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font",
  "terminal.integrated.fontLigatures.enabled": true,
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.fontWeight": "600",
  "editor.linkedEditing": true,
  "editor.mouseWheelZoom": true,
  "explorer.confirmDelete": false,
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.overtypeCursorStyle": "line",
  "editor.cursorBlinking": "expand",
  "accessibility.signalOptions.volume": 0,
  "workbench.startupEditor": "none",
  "window.restoreWindows": "none",
  "workbench.editor.enablePreview": false,
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "explorer.compactFolders": false,
  "window.menuBarVisibility": "compact",
  "workbench.navigationControl.enabled": false,
  "chat.commandCenter.enabled": false,
  "window.commandCenter": false,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorStyleInactive": "line",
  "workbench.statusBar.visible": false,
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "workbench.iconTheme": "catppuccin-mocha",
  "workbench.sideBar.location": "right",
  "[snippets]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "workbench.colorTheme": "Catppuccin Mocha"
}
